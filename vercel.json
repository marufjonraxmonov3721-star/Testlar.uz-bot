{
  "version": 2,
  "crons": [
    {
      "path": "/api/ping",
      "schedule": "*/10 * * * *"
    }
  ],
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/ping",
      "dest": "api/ping.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
